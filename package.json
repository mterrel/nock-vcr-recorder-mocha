{
  "name": "mocha-nock",
  "version": "0.2.6",
  "description": "Easy recording and playback of HTTP(S) for Mocha.js using Nock.",
  "main": "dist/describe-fixture.js",
  "scripts": {
    "build": "tsc",
    "test": "npm run build && rm -rf test/fixtures && node_modules/mocha/bin/mocha test/*-test.js ",
    "manual-release": "semantic-release --no-ci",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unboundedsystems/mocha-nock.git"
  },
  "keywords": [
    "mocha",
    "nock",
    "testing",
    "fixtures",
    "mocking"
  ],
  "author": "Mark Terrel",
  "contributors": [
    "Jake Craige <jake@poeticsystems.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unboundedsystems/mocha-nock/issues"
  },
  "dependencies": {
    "fs-extra": "^8.0.0",
    "lodash.assign": "^4.2.0",
    "mkdirp": "^0.5.0",
    "nock": "^10.0"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "6.3.3",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/npm": "5.3.5",
    "@semantic-release/release-notes-generator": "7.3.5",
    "@types/fs-extra": "8.0.1",
    "@types/lodash.assign": "4.2.6",
    "@types/mkdirp": "0.5.2",
    "@types/mocha": "5.2.5",
    "@types/nock": "10.0.3",
    "@types/node": "10.17.13",
    "express": "4.17.1",
    "mocha": "3.5.3",
    "proxyquire": "2.1.3",
    "request": "2.88.0",
    "semantic-release": "15.14.0",
    "typescript": "3.7.5"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/git"
  ]
